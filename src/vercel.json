{
    "version": 2,
    "builds": [
        {
            "src": "index,js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dist": "dist/index.html"}]
}